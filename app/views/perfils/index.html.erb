<p id="notice"><%= notice %></p>

<h1>Listing Perfils</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Direcion</th>
      <th>Altitud</th>
      <th>Longitud</th>
      <th>Description</th>
      <th>Mision</th>
      <th>Vision</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @perfils.each do |perfil| %>
      <tr>
        <td><%= perfil.name %></td>
        <td><%= perfil.direcion %></td>
        <td><%= perfil.altitud %></td>
        <td><%= perfil.longitud %></td>
        <td><%= perfil.description %></td>
        <td><%= perfil.mision %></td>
        <td><%= perfil.vision %></td>
        <td><%= perfil.user %></td>
        <td><%= link_to 'Show', perfil %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


